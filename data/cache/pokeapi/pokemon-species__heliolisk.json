      "version": {
        "name": "sword",
        "url": "https://pokeapi.co/api/v2/version/33/"
      }
    },
    {
      "flavor_text": "Venerado por una antigua civilización del\ndesierto, ahora desaparecida, llegó a Galar\njunto a sus tesoros y reliquias.",
      "language": {
        "name": "es",
        "url": "https://pokeapi.co/api/v2/language/7/"
